# Discord Bot Token
TOKEN=your-discord-bot-token-here

# Discord Application & Server IDs
CLIENT_ID=your-client-id-here
GUILD_ID=your-guild-id-for-command-deploy

# MongoDB connection string
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority

# Channel ID where the bot posts the respawn list
STATUS_CHANNEL_ID=your-status-channel-id

# Roles (case-sensitive)
ROLE_USER=your-claim-role-name
ROLE_ADMIN=your-admin-role-name

# Default Tibia world to validate characters
TIBIA_WORLD=YourTibiaWorld
